<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <mapper namespace="com.woo.s1.product.ProductDAO">
  	<select id="getProductList" resultType="com.woo.s1.product.ProductDTO">
  		SELECT PRODUCTNUM, PRODUCTNAME, PRODUCTJUMSU
  		FROM PRODUCT
  		ORDER BY PRODUCTNUM DESC
  	</select>
  	
  	<select id="getProductDetail" parameterType="com.woo.s1.product.ProductDTO" resultType="com.woo.s1.product.ProductDTO">
  		SELECT * FROM PRODUCT WHERE PRODUCTNUM=#{productNum}
  	</select>
  
  </mapper>