<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <mapper namespace="com.woo.s1.member.MemberDAO">
  	
  	<select id="getMemberDetail" parameterType="MemberDTO" resultType="MemberDTO">
  		SELECT * FROM MEMBER WHERE ID=#{id}
  	</select>
  	
  	<insert id="setMemberAdd" parameterType="MemberDTO">
  		INSERT INTO MEMBER (ID, PW, MEMBERNAME, MEMBERPHONE, EMAIL)
  		VALUES (#{id}, #{pw}, #{name}, null, null)
  	</insert>
  	
  	<select id="getMemberList" resultType="MemberDTO">
  		SELECT * FROM MEMBER
  	</select>
  	
  </mapper>