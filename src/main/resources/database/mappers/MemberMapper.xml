<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <mapper namespace="com.woo.s1.member.MemberDAO">
  	
  	<select id="getMemberDetail" parameterType="MemberDTO" resultType="com.woo.s1.member.MemberDTO">
  		
  	</select>
  	
  	<insert id="setMemberAdd" parameterType="MemberDTO">
  		INSERT INTO MEMBER (ID, PW, NAME, ADDRESS, PHONE, EMAIL)
  		VALUES (#{id}, #{pw}, #{name}, #{address}, #{phone}, #{email})
  	</insert>
  	
  	
  	
  	
  	
  </mapper>